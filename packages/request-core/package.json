{
  "name": "request-core",
  "version": "0.0.1",
  "description": "请求库核心层 - 定义接口和实现与具体请求库无关的高级功能",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc && tsc --module commonjs --moduleResolution node --outDir dist/cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --moduleResolution node --outDir dist/cjs",
    "dev": "tsc --watch",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "~5.9.2"
  },
  "keywords": [
    "request",
    "http",
    "browser",
    "typescript",
    "cache",
    "retry"
  ],
  "author": "",
  "license": "ISC"
}
